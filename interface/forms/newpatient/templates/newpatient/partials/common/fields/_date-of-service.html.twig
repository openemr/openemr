{# Date Of Service #}
<div class="col-sm {{ displayOptionClass('enc_service_date') }}">
    <div class="mb-3">
        <label for="form_date" class="text-end">{{ "Date of Service:"|xlt }}</label>
        <input type="text"
               class="form-control datepicker"
               name="form_date"
               id="form_date"
               value="{{ encounter.date|oeFormatDateTime|attr }}" />
    </div>
</div>
