{
    "name": "openemr/oe-module-gcip-auth",
    "description": "Google Cloud Identity Platform (GCIP) authentication module for OpenEMR",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": ["openemr", "gcip", "authentication", "google", "oauth2", "sso"],
    "homepage": "https://www.open-emr.org",
    "authors": [
        {
            "name": "OpenEMR Development Team",
            "email": "info@open-emr.org",
            "homepage": "https://www.open-emr.org"
        }
    ],
    "require": {
        "php": ">=8.1",
        "firebase/php-jwt": "^6.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "OpenEMR\\Modules\\GcipAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEMR\\Modules\\GcipAuth\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "analyse": "phpstan analyse src tests --level=8"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}