#!/bin/sh
# Possible options are prod or dev. No spaces, no quotes.
export OPENEMR__ENVIRONMENT=
# Possible options are true to prevent dated reminders and background apps.Else empty or false. No spaces, no quotes.
export OPENEMR__NO_BACKGROUND_TASKS=

# Front controller routing (optional, disabled by default)
# Set to 1 to enable centralized routing and block .inc.php files
export OPENEMR_ENABLE_FRONT_CONTROLLER=

# Front controller logging: debug or empty
export OPENEMR_FC_LOG_LEVEL=
