    "require-dev": {
        "ergebnis/composer-normalize": "^2.48",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/php-code-coverage": "^11.0",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^2.1",
        "squizlabs/php_codesniffer": "^4.0",
        "symfony/panther": "^2.0",
        "zircote/swagger-php": "^3.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/openemr/wkhtmltopdf-openemr"
        },
        {
            "type": "git",
            "url": "https://github.com/openemr/oe-module-cqm"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenEMR\\": "src"
        },
        "classmap": [
            "library/classes"
        ],
        "files": [
            "library/htmlspecialchars.inc.php",
            "library/formdata.inc.php",
            "library/sanitize.inc.php",
            "library/formatting.inc.php",
            "library/date_functions.php",
            "library/validation/validate_core.php",
            "library/translation.inc.php"
        ],
        "exclude-from-classmap": [
            "src/Common/Compatibility/Checker.php",
            "library/classes/ClinicalTypes/",
            "library/classes/rulesets/",
            "library/classes/smtp/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEMR\\Tests\\": "tests\\Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "openemr/oe-module-installer-plugin": true,
            "php-http/discovery": false,
            "phpstan/extension-installer": true
        },
        "platform": {
            "ext-calendar": "8.2",
            "ext-ctype": "8.2",
            "ext-curl": "8.2",
            "ext-fileinfo": "8.2",
            "ext-gd": "8.2",
            "ext-iconv": "8.2",
            "ext-intl": "8.2",
            "ext-json": "8.2",
            "ext-ldap": "8.2",
            "ext-mbstring": "8.2",
            "ext-mysqli": "8.2",
            "ext-openssl": "8.2",
            "ext-pdo": "8.2",
            "ext-pdo_mysql": "8.2",
            "ext-phar": "8.2",
            "ext-session": "8.2",
            "ext-simplexml": "8.2",
            "ext-soap": "8.2",
            "ext-sockets": "8.2",
            "ext-sodium": "8.2",
            "ext-tokenizer": "8.2",
            "ext-xml": "8.2",
            "ext-xmlreader": "8.2",
            "ext-xmlwriter": "8.2",
            "ext-xsl": "8.2",
            "ext-zip": "8.2",
            "ext-zlib": "8.2",
            "php": "8.2"
        }
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        }
    },
    "scripts": {
        "checks": [
            "composer validate",
            "composer normalize --dry-run"
        ]
    }
}
