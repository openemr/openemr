    "require-dev": {
        "ergebnis/composer-normalize": "^2.48",
        "maglnet/composer-require-checker": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/php-code-coverage": "^11.0",
        "phpunit/phpunit": "^11.0",
        "ramsey/conventional-commits": "^1.5",
        "rector/rector": "^2.1",
        "shipmonk/phpstan-baseline-per-identifier": "^2.3",
        "squizlabs/php_codesniffer": "^4.0",
        "symfony/panther": "^2.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/openemr/wkhtmltopdf-openemr"
        },
        {
            "type": "git",
            "url": "https://github.com/openemr/oe-module-cqm"
        }
    ],
    "autoload": {
        "psr-4": {
            "OpenEMR\\": "src"
        },
        "classmap": [
            "library/classes"
        ],
        "files": [
            "library/htmlspecialchars.inc.php",
            "library/formdata.inc.php",
            "library/sanitize.inc.php",
            "library/formatting.inc.php",
            "library/date_functions.php",
            "library/validation/validate_core.php",
            "library/translation.inc.php"
        ],
        "exclude-from-classmap": [
            "src/Common/Compatibility/Checker.php",
            "library/classes/ClinicalTypes/",
            "library/classes/rulesets/",
            "library/classes/smtp/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OpenEMR\\PHPStan\\Rules\\": ".phpstan",
            "OpenEMR\\Tests\\": "tests\\Tests",
            "OpenEMR\\Modules\\SSO\\": "interface/modules/custom_modules/oe-module-sso/src"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "openemr/oe-module-installer-plugin": true,
            "php-http/discovery": false,
            "phpstan/extension-installer": true
        },
        "platform": {
            "ext-bcmath": "8.2",
            "ext-calendar": "8.2",
            "ext-ctype": "8.2",
            "ext-curl": "8.2",
            "ext-dom": "8.2",
            "ext-fileinfo": "8.2",
            "ext-filter": "8.2",
            "ext-gd": "8.2",
            "ext-iconv": "8.2",
            "ext-imagick": "8.2",
            "ext-intl": "8.2",
            "ext-json": "8.2",
            "ext-ldap": "8.2",
            "ext-libxml": "8.2",
            "ext-mbstring": "8.2",
            "ext-mysqli": "8.2",
            "ext-openssl": "8.2",
            "ext-pdo": "8.2",
            "ext-pdo_mysql": "8.2",
            "ext-phar": "8.2",
            "ext-session": "8.2",
            "ext-simplexml": "8.2",
            "ext-soap": "8.2",
            "ext-sockets": "8.2",
            "ext-sodium": "8.2",
            "ext-tokenizer": "8.2",
            "ext-xml": "8.2",
            "ext-xmlreader": "8.2",
            "ext-xmlwriter": "8.2",
            "ext-xsl": "8.2",
            "ext-zip": "8.2",
            "ext-zlib": "8.2",
            "php": "8.2"
        },
        "sort-packages": true
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        },
        "ramsey/conventional-commits": {
            "config": {
                "bodyRequired": false,
                "bodyWrapWidth": 72,
                "descriptionCase": null,
                "descriptionEndMark": null,
                "requiredFooters": [],
                "scopeCase": "kebab",
                "scopeRequired": false,
                "typeCase": "lower",
                "types": [
                    "build",
                    "chore",
                    "ci",
                    "docs",
                    "feat",
                    "fix",
                    "perf",
                    "refactor",
                    "revert",
                    "style",
                    "test"
                ]
            }
        }
    },
    "scripts": {
        "checks": [
            "composer validate --strict",
            "composer normalize --dry-run"
        ],
        "code-quality": [
            "@conventional-commits:check",
            "@php-syntax-check",
            "@phpcbf",
            "@phpcs",
            "@phpstan",
            "@rector-check",
            "@require-checker"
        ],
        "conventional-commits:check": "./vendor/bin/conventional-commits validate",
        "php-syntax-check": "git ls-files -z '*.inc' '*.php' | xargs -0 php -l | grep -vF 'No syntax errors detected in'",
        "phpcbf": "php -d memory_limit=4g ./vendor/bin/phpcbf --standard=ci/phpcs.xml .",
        "phpcs": "php -d memory_limit=4g ./vendor/bin/phpcs --standard=ci/phpcs.xml .",
        "phpstan": "phpstan analyze --memory-limit=4G --configuration=phpstan.neon.dist",
        "phpstan-baseline": [
            "phpstan analyze --memory-limit=8G --configuration=phpstan.neon.dist --generate-baseline=.phpstan/baseline/loader.php",
            "split-phpstan-baseline .phpstan/baseline/loader.php"
        ],
        "rector-check": "php -d memory_limit=4g ./vendor/bin/rector process --dry-run",
        "rector-fix": "php -d memory_limit=4g ./vendor/bin/rector process",
        "require-checker": "php -d memory_limit=4g ./vendor/bin/composer-require-checker check --config-file=.composer-require-checker.json"
    }
}
